import pandas as pd
import requests
import json
import sys
import os
import csv

outPutcolumnName=[]
columnName=""
separator = []
keywords =[]
inputFile= ""
outputFile =""
rateOfSimlarity =0.0
roughList=[]
fineList = []

def setCommand (list)

    for a in range(len(list)):
	    if list[a] == "-s":
		    separator.append(list[a+1])
	    if list[a] == "-k":
		    keywords.append(list[a+1])
	    if list[a] == "-i":
	        inputFile = list[a+1]
	    if list[a] == "-o":
		    outputFile = list[a+1]
	    if list[a] == "-r":
	        rateOfSimlarity = float(list[a+1])
	    if list[a] == "-c":
	        columnName = list[a+1]
	    if sys.argv[a] =="-oc":
	        list.append(list[a+1])

def commandSeparatorTest:
    command = ["-s",";"]
    setCommand(command)
    assert separator[0]=";"

def commandKeywordsTest:
    command = ["-k","College"]
    setCommand(command)
    assert keywords[0]="College"

def commandSeparatorTest:
    command = ["-o","someFile.csv"]
    setCommand(command)
    assert outputFile ="someFile.csv"

def commandKeywordsTest:
    command = ["-r","0.8"]
    setCommand(command)
    assert rateOfSimlarity= 0.8

if __name__ == "__main__":
    commandSeparatorTest
    commandKeywordsTest
    print("Everything passed")
